#################################################
config("ohos_glue_config") {
  defines = [ "OHOS_WEBVIEW_GLUE" ]
  if (is_asan || use_hwasan) {
    defines += [ "IS_ASAN" ]
  }

  if (target_cpu == "arm64") {
    defines += [ "webview_arm64" ]
  } else if (target_cpu == "arm") {
    defines += [ "webview_arm" ]
  } else if (target_cpu == "x86_64") {
    defines += [ "webview_x86_64" ]
  }

  include_dirs = [
    "../ohos_nweb",
    "${target_gen_dir}",
  ]
}

ohos_shared_library("ohos_base_glue_source") {
  output_name = "arkweb_glue_base"

  branch_protector_ret = "pac_ret"

  configs = [ ":ohos_glue_config" ]

  sources = get_target_outputs(":ohos_glue_base_prepare")

  deps = [ ":ohos_glue_base_prepare" ]

  external_deps = [ "hilog:libhilog" ]

  part_name = "webview"
  subsystem_name = "web"
}

ohos_shared_library("ohos_nweb_glue_source") {
  output_name = "arkweb_core_loader_glue"

  branch_protector_ret = "pac_ret"

  configs = [ ":ohos_glue_config" ]

  sources = get_target_outputs(":ohos_glue_nweb_prepare")

  deps = [
    ":ohos_base_glue_source",
    ":ohos_glue_nweb_prepare",
  ]

  external_deps = [
    "hilog:libhilog",
    "ability_runtime:ability_connect_callback_stub",
  ]

  part_name = "webview"
  subsystem_name = "web"
}

ohos_shared_library("ohos_adapter_glue_source") {
  branch_protector_ret = "pac_ret"

  configs = [ ":ohos_glue_config" ]

  sources = get_target_outputs(":ohos_glue_adapter_prepare")

  deps = [
    ":ohos_base_glue_source",
    ":ohos_glue_adapter_prepare",
    "../ohos_adapter:nweb_ohos_adapter",
  ]

  external_deps = [
    "hilog:libhilog",
    "ability_runtime:ability_connect_callback_stub",
  ]

  part_name = "webview"
  subsystem_name = "web"
}
