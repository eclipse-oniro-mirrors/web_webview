import("//build/test.gni")
import("../../../web_aafwk.gni")

module_output_path = "webview/webview"

ohos_unittest("nweb_helper_test") {
  module_out_path = module_output_path

  sources = [ "nweb_hisysevent_test.cpp" ]

  include_dirs = [ "../common" ]

  deps = [
    "$webview_path/ohos_nweb:web_configs",
    "$webview_path/ohos_nweb:libnweb"
  ]

  external_deps = [
    "googletest:gmock_main",
    "googletest:gtest_main"
  ]
}

group("unittest") {
  testonly = true
  deps = [ ":nweb_hisysevent_test" ]
}
