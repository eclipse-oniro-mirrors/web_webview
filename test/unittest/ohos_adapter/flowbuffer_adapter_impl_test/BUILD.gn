import("//build/test.gni")

module_output_path = "web/webview"

config("module_private_config") {
  visibility = [ ":*" ]

  include_dirs = [ "../../../../ohos_adapter/flowbuffer_adapter/include" ]
}

ohos_unittest("flowbuffer_adapter_impl_test") {
  module_out_path = module_output_path

  sources = [ "flowbuffer_adapter_impl_test.cpp" ]

  configs = [ ":module_private_config" ]

  deps = [ "../../../../ohos_adapter:nweb_ohos_adapter" ]

  external_deps = [
    "c_utils:utils",
    "eventhandler:libeventhandler",
    "hilog:libhilog",
  ]
}

group("unittest") {
  testonly = true

  deps = [ ":flowbuffer_adapter_impl_test" ]
}
